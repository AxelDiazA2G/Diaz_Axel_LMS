<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="Introduction_Screen">
  <grid id="mainGrid" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="10" vgap="10">
    <margin top="10" left="10" bottom="10" right="10"/>
    <constraints>
      <xy x="20" y="20" width="1842" height="1094"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="subGrid" binding="panel1" layout-manager="GridLayoutManager" row-count="6" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="10" vgap="10">
        <margin top="20" left="20" bottom="20" right="20"/>
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties/>
        <border type="line"/>
        <children>
          <component id="titleLabel" class="javax.swing.JLabel">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="3" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="28"/>
              <text value="LMS: Library Management System by Axel Diaz"/>
            </properties>
          </component>
          <component id="welcomeLabel" class="javax.swing.JLabel">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="14"/>
              <text value="Welcome to the LMS program. Would you like to load the books? Select an option from below:"/>
            </properties>
          </component>
          <component id="imageLabel" class="javax.swing.JLabel">
            <constraints>
              <grid row="1" column="2" row-span="4" col-span="1" vsize-policy="3" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <icon value="Intro.png"/>
            </properties>
          </component>
          <component id="loadFileRadio" class="javax.swing.JRadioButton" binding="loadFileRadioButton">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Load File"/>
            </properties>
          </component>
          <component id="loadTestRadio" class="javax.swing.JRadioButton" binding="loadTestCasesRadioButton">
            <constraints>
              <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Load Test Cases"/>
            </properties>
          </component>
          <component id="continueButton" class="javax.swing.JButton" binding="button1">
            <constraints>
              <grid row="4" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="16"/>
              <text value="Continue"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
  <buttonGroups>
    <group name="buttonGroup1">
      <member id="loadFileRadio"/>
      <member id="loadTestRadio"/>
    </group>
  </buttonGroups>
</form>
